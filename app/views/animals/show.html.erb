<h1><%= @animal.name %>'s profile</h1>

<p><%= @animal.address %></p>
<p><%= @animal.species %></p>
<p>Found by: <%= @animal.user&.username %> | Tel : <%= @animal.user&.telephone %></p>


<%= link_to "Delete", animal_path(@animal), data: {turbo_method: :delete, turbo_confirm: "Delete #{@animal.name} ?"}  %> |
<%= link_to "Edit", edit_animal_path(@animal) %> |
<%= link_to "Return to all", animals_path  %>
